    <div class="container">
      <% rows.map((eachData) => { %>

    
        <div class="card mb-3" style="max-width: 540px;">
            <div class="row g-0">
              <div class="col-md-8">
                <div class="card-body">
                  <h5 class="card-title"><%= eachData.RegisterNo %></h5>
                  <h5 class="card title"><%= eachData.email %></h5>
                  <h5 class="card-title"><%= eachData.FirstName %></h5>
                </div>
              </div>
            </div>
          </div>
          <% }) %>
    </div>

