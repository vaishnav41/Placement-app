          </a>
    
          <ul class="nav nav-pills">
            <li class="nav-item"><a href="#" class="nav-link " aria-current="page"><%= entry.RegisterNo %></a></li>
            <form action="http://localhost:4020/Logoutstudent" method="get">
              <button type="submit" class="btn btn-warning">
